{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "projectType": "application",
  "sourceRoot": "apps/adam",
  "targets": {
    "build": {
      "executor": "@nx-go/nx-go:build",
      "options": {
        "outputPath": "dist/apps/adam",
        "main": "apps/adam/main.go"
      }
    },
    "serve": {
      "executor": "@nx-go/nx-go:serve",
      "options": {
        "main": "apps/adam/main.go"
      }
    },
    "test": {
      "executor": "./tools/executors/ginkgo:test",
      "options": {
        "ci": true,
        "ignoreExtra": true
      }
    },
    "lint": {
      "executor": "@nx-go/nx-go:lint"
    },
    "generate": {
      "executor": "./tools/executors/gqlgen:go",
      "options": {
        "config": "gqlgen.yml"
      }
    },
    "docker": {
      "executor": "nx:run-commands",
      "options": {
        "command": "docker build -t coffeewithegg/adam -f apps/adam/Dockerfile ."
      }
    }
  },
  "tags": []
}
